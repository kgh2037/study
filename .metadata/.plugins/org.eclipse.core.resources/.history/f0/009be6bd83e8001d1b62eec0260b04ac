package study2;

public class WhileTest {

	public static void main(String[] args) {
		int a = 1;
		while(a<=5) {
			System.out.println(a+ "");
			++a;
		}
		int a = 10;
		while(a>=1) {
			System.out.println(a+"");
			--a;
		}
		int[] month = {1,3,5,10,12};
		int idx = 0;
		while(idx < months.length) {
			System.out.println(month[idx] + "ì›” ");
			cnt++;
		}
	}
}